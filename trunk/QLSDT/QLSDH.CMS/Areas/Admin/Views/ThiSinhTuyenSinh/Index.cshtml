        } else {
            $("#gv2_khoa").show();
            $("#gv2_ten").show();
            $("#gv2_ten2").hide();
            $("#gv2_coquancongtac").hide();
        }
    });

    $('#ddlKhoa_1').on('change', function (e) {
        var khoaid = this.value;
        loadGV_V2(khoaid, 'ddlGV_1');
    });
