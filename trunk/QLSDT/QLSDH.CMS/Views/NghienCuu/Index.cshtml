<script type="text/javascript">
    $(document).ready(function () {
        $('#btnUpload').click(function () {
            var id = $('#hdfId').val();
            var mancs = $('#hdfMaNCS').val();
            $("#divProcessing").show();
            if (window.FormData !== undefined) {
                var fileUpload = $("#fupload").get(0);
                var files = fileUpload.files;
                var fileData = new FormData();
                for (var i = 0; i < files.length; i++) {
                    fileData.append(files[i].name, files[i]);
                }
                fileData.append('hdfId', id);
                fileData.append('hdfMaNCS', mancs);
                $.ajax({
                    url: '@Url.Action("UploadHuongDan", "NghienCuu")',
                    type: "POST",
                    contentType: false,
                    processData: false,
                    data: fileData,
                    success: function (data) {
                        $("#divProcessing").hide();
                        if (data == 'OK') {
                            setTimeout(function () { location.reload(); }, 2000);
                            toastr.success('Upload hồ sơ thành công');
                        } else {
                            toastr.error(data);
                        }
                    },
                    error: function (err) {
                        alert(err.statusText);
                    }
                });
            }
        });
    });
    function uploadFile(id, mancs) {
        $('#hdfId').val(id);
        $('#hdfMaNCS').val(mancs);
        $('#modelupload').modal({ backdrop: 'static', keyboard: false })
    }
</script>
